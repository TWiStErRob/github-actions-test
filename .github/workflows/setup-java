name: Java Versions
on:
  push:
  workflow_dispatch:
jobs:
  v1:
    name: setup-java@v1
    runs-on: ubuntu-latest
    steps:
      - name: Set up JDK 6
        uses: actions/setup-java@v1
        with:
          java-version: 6
      - name: Set up JDK 7
        uses: actions/setup-java@v1
        with:
          java-version: 7
  v2:
    name: setup-java@v2
    runs-on: ubuntu-latest
    steps:
      - name: Set up JDK 6
        uses: actions/setup-java@v2
        with:
          java-version: 6
      - name: Set up JDK 7
        uses: actions/setup-java@v2
        with:
          java-version: 7
  v3:
    name: setup-java@v3
    runs-on: ubuntu-latest
    steps:
      - name: Set up JDK 6
        uses: actions/setup-java@v3
        with:
          java-version: 6
      - name: Set up JDK 7
        uses: actions/setup-java@v3
        with:
          java-version: 7
